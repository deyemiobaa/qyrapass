<tr id="<%= dom_id member %>">
  <td class="p-4 border-b border-slate-200">
    <p class="text-sm font-semibold text-slate-700"><%= member.name %></p>
  </td>
  <td class="p-4 border-b border-slate-200">
    <p class="text-sm font-semibold text-slate-700"><%= member.email %></p>
  </td>
  <td class="p-4 border-b border-slate-200">
    <p class="text-sm font-semibold text-slate-700"><%= member.phone %></p>
  </td>
  <td class="p-4 border-b border-slate-200">
    <p class="text-sm text-slate-500"><%= member.last_signed_in&.strftime("%d %b, %Y")
 %></p>
  </td>
  <td class="p-4 border-b border-slate-200">
    <p class="text-sm text-slate-500"><%= member.access_expiry_date&.strftime("%d %b, %Y")
 %></p>
  </td>
  <td class="p-4 border-b border-slate-200">
    <p class="text-sm text-slate-500">
      <% if member.is_active? %>
        <span class="text-center px-2 py-1 font-sans text-xs font-bold text-green-900 uppercase rounded-md select-none whitespace-nowrap bg-green-500/20">Active</span>
      <% else %>
        <span class="text-center px-2 py-1 font-sans text-xs font-bold uppercase rounded-md select-none whitespace-nowrap bg-red-300/70 text-red-900">Inactive</span>
      <% end %>
    </p>
  </td>
  <td class="p-4 border-b border-slate-200">
    <div class="flex gap-6 items-center">
      <%= link_to edit_member_path(member) do %>
        <button
          class="h-10 max-h-[40px] w-10 select-none rounded-lg text-center font-sans text-xs font-medium uppercase text-slate-900 transition-all hover:bg-slate-900/10 active:bg-slate-900/20 cursor-pointer flex justify-center items-center"
          type="button">
          <span>
            <svg
              xmlns="http://www.w3.org/2000/svg"
              viewBox="0 0 24 24"
              fill="currentColor"
              aria-hidden="true"
              class="w-4 h-4">
              <path
                d="M21.731 2.269a2.625 2.625 0 00-3.712 0l-1.157 1.157 3.712 3.712 1.157-1.157a2.625 2.625 0 000-3.712zM19.513 8.199l-3.712-3.712-12.15 12.15a5.25 5.25 0 00-1.32 2.214l-.8 2.685a.75.75 0 00.933.933l2.685-.8a5.25 5.25 0 002.214-1.32L19.513 8.2z"
              ></path>
            </svg>
          </span>
        </button>
      <% end %>
      <%= link_to member_path(member), data: { turbo_confirm: "Are you sure?", turbo_method: :delete } do %>
        <button
          class="h-10 max-h-[40px] w-10 select-none rounded-lg text-center font-sans text-xs font-medium uppercase text-slate-900 transition-all hover:bg-slate-900/10 active:bg-slate-900/20 cursor-pointer flex justify-center items-center"
          type="button">
          <span>
            <svg
              xmlns="http://www.w3.org/2000/svg"
              fill="none"
              viewBox="0 0 24 24"
              stroke-width="2"
              stroke="currentColor"
              class="h-4 w-4">
              <path
                stroke-linecap="round"
                stroke-linejoin="round"
                d="M14.74 9l-.346 9m-4.788 0L9.26 9m9.968-3.21c.342.052.682.107 1.022.166m-1.022-.165L18.16 19.673a2.25 2.25 0 01-2.244 2.077H8.084a2.25 2.25 0 01-2.244-2.077L4.772 5.79m14.456 0a48.108 48.108 0 00-3.478-.397m-12 .562c.34-.059.68-.114 1.022-.165m0 0a48.11 48.11 0 013.478-.397m7.5 0v-.916c0-1.18-.91-2.164-2.09-2.201a51.964 51.964 0 00-3.32 0c-1.18.037-2.09 1.022-2.09 2.201v.916m7.5 0a48.667 48.667 0 00-7.5 0"></path>
            </svg>
          </span>
        </button>
      <% end %>
    </div>
  </td>
</tr>
